<?xml version="1.0"?>
<odoo>
<data>
    
    
    <act_window id="ranchy.action_window" name="ranchy" res_model="members.ranchy" view_mode="tree,form"/>
    
    <menuitem action="ranchy.action_window" name="ranchy" id="ranchy.menu_root" sequence ="450"/>
    <menuitem action="ranchy.action_window" name="Members" id="ranchy.members" parent="ranchy.menu_root"/>
    
    
    <record id= "ranchy_members_template_form_view" model= "ir.ui.view">
   <field name="inherit_id" ref="base.view_partner_form" />
   <field name="model">members.ranchy</field >
   <field name="arch" type= "xml">
         <field name="list_price" position="after">
           
             <field name="member_firstname"/>
             <field name="member_lastname"/>
             <field name="member_addresss"/>
             <field name="member_pic" widget="image" class="oe_avatar" options="{&quot;preview_image&quot;: &quot;image_medium&quot;, &quot;size&quot;: [90, 90]}"/>
           
           
         </field>
   </field>
   </record>
    
</data>
</odoo>